# Snowflake Configuration Variables

# Snowflake account identifier
SNOWFLAKE_ACCOUNT=

# Snowflake username
SNOWFLAKE_USERNAME=

# Snowflake password
SNOWFLAKE_PASSWORD=

# Snowflake warehouse to use
SNOWFLAKE_WAREHOUSE=

# Snowflake database name
SNOWFLAKE_DATABASE=

# Snowflake schema name
SNOWFLAKE_SCHEMA=

# Connection pooling settings
SNOWFLAKE_MAX_CONNECTIONS=5

# Query timeout in seconds
SNOWFLAKE_QUERY_TIMEOUT=60

# SSL/TLS settings
SNOWFLAKE_USE_SSL=true

# Environment (development, staging, production)
NODE_ENV=development
